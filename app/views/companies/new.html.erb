

<h1>Please Create a New Company, Welcome to the Platform!</h1>

<%= form_for @company do |u|%>
  <%= u.label :name%>
  <%= u.text_field :name%>
  <br>
  <%= u.label :address %>
  <%= u.text_field :address%>
  <br>
  <%= u.label :phone_number %>
  <%= u.text_field :phone_number %>

  <br>
  <%= u.submit%>
<%end%>